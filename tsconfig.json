  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext, commonjs",
    "jsx": "react",
    "outDir": "./dist/",
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
